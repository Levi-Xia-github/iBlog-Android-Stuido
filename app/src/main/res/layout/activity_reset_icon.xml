<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResetIconActivity">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/reset_icon"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <com.loopj.android.image.SmartImageView
        android:id="@+id/reset_icon_a"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="5dp"
        android:scaleType="centerCrop">

    </com.loopj.android.image.SmartImageView>

    <com.loopj.android.image.SmartImageView
        android:id="@+id/reset_icon_b"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_below="@+id/textView4"
        android:layout_marginStart="62dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/reset_icon_a"
        android:scaleType="centerCrop">

    </com.loopj.android.image.SmartImageView>

    <com.loopj.android.image.SmartImageView
        android:id="@+id/reset_icon_c"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_below="@+id/reset_icon_a"
        android:layout_alignParentStart="true"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="5dp"
        android:scaleType="centerCrop">

    </com.loopj.android.image.SmartImageView>

    <com.loopj.android.image.SmartImageView
        android:id="@+id/reset_icon_d"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_below="@+id/reset_icon_b"
        android:layout_marginStart="62dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/reset_icon_c"
        android:scaleType="centerCrop">

    </com.loopj.android.image.SmartImageView>

    <com.loopj.android.image.SmartImageView
        android:id="@+id/reset_icon_e"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_below="@+id/reset_icon_c"
        android:layout_alignParentStart="true"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="5dp"
        android:scaleType="centerCrop">

    </com.loopj.android.image.SmartImageView>

    <com.loopj.android.image.SmartImageView
        android:id="@+id/reset_icon_f"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_below="@+id/reset_icon_d"
        android:layout_marginStart="62dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/reset_icon_e"
        android:scaleType="centerCrop">

    </com.loopj.android.image.SmartImageView>

    <com.loopj.android.image.SmartImageView
        android:id="@+id/reset_icon_g"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_below="@+id/reset_icon_e"
        android:layout_alignParentStart="true"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="5dp"
        android:scaleType="centerCrop">

    </com.loopj.android.image.SmartImageView>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="12dp"
        android:text="请在下面选择你喜欢的头像(点击选择）"
        android:textSize="15sp" />

</RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>